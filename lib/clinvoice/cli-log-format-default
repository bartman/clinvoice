#autoload -U
#vim: set ft=zsh 
#
# this function outputs the daily itemized output

for day in ${(k)daytext} ; do
        local time=${dayhours[$day]}

        echo "$day  (${time}h)"

        lines=( ${(s::Q)daytext[$day]} )
        for n in $lines ; do 
                echo "    $n"
        done
done

